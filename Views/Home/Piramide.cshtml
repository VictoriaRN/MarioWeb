@model int

@{
  var cantidad = Model;
  var alt = Model;
}
  
      @for(int i = 0; i < Model ; i++) {

        @for(int x=0; x<cantidad-1; x++){
		    <img  src="~/images/blanco.webp" alt="#">               
		     }
         

	      @for(int s=0; s<alt-Model+1;  s++){
	      <img  src="~/images/bloque.webp" alt="#">
	  	   }

         @for(int x=0; x<=Model-Model; x++){
		    <img  src="~/images/blanco.webp" alt="#">               
		     }

         @for(int s=0; s<alt-Model+1;  s++){
	      <img  src="~/images/bloque.webp" alt="#">
	  	   }
      <br/>
       cantidad--;
       alt++;
       }   
    <br/>
    
    
    
    



